
dialect "java"


import org.optaplanner.core.api.score.buildin.simple.SimpleScoreHolder;
import domain.Cell

global SimpleScoreHolder scoreHolder;

    
rule "rows"
    when
        $cell_1 : Cell();
        $cell_2 : Cell();

    then
        System.out.println("Look at all these cells: " + $cell_1);
        scoreHolder.addConstraintMatch(kcontext, -100);
end